{
  "name": "react-axl",
  "version": "0.0.2",
  "description": "React components for rendering Phoenix Contact axioline products",
  "main": "dist/react-axl.umd.js",
  "files": [
    "dist",
    "src",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "build:lib": "NODE_ENV=production webpack --mode production",
    "eject": "react-scripts eject",
    "test": "react-scripts test",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "author": "Zachary Mink",
  "license": "MIT",
  "dependencies": {
    "classnames": "^2.2.6",
    "prop-types": "^15.7.2",
    "react-motion": "^0.5.2",
    "unit-to-px": "^1.0.5",
    "use-viewport-sizes": "^0.2.7"
  },
  "devDependencies": {
    "@babel/core": "^7.9.6",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/preset-env": "^7.9.6",
    "@babel/preset-react": "^7.9.4",
    "@storybook/addon-actions": "^5.3.18",
    "@storybook/addon-knobs": "^5.3.18",
    "@storybook/addon-links": "^5.3.18",
    "@storybook/addons": "^5.3.18",
    "@storybook/react": "^10.0.2",
    "babel-jest": "^26.0.1",
    "babel-loader": "^8.1.0",
    "css-loader": "^3.5.2",
    "eslint-plugin-react-hooks": "^3.0.0",
    "file-loader": "^6.2.0",
    "identity-obj-proxy": "^3.0.0",
    "mini-css-extract-plugin": "^0.9.0",
    "webpack": "^4.46.0",
    "webpack-cli": "^4.10.0",
    "react": "16.14.0",
    "react-dom": "16.14.0",
    "react-draggable": "^4.4.3",
    "react-scripts": "^5.0.1",
    "react-svg-loader": "^3.0.3",
    "react-test-renderer": "^16.13.1",
    "sass": "^1.93.2",
    "sass-loader": "^7.3.1",
    "style-loader": "^1.1.4",
    "svg-inline-loader": "^0.8.2"
  },
  "peerDependencies": {
    "react": "16.x",
    "react-dom": "16.x"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(css|less|scss)$": "identity-obj-proxy",
      "@assets(.*)$": "<rootDir>/src/assets/$1",
      "@components(.*)$": "<rootDir>/src/components/$1",
      "@controllers(.*)$": "<rootDir>/src/components/Controllers/$1",
      "@core(.*)$": "<rootDir>/src/components/Core/$1",
      "@modules(.*)$": "<rootDir>/src/components/Modules/$1",
      "@utils(.*)$": "<rootDir>/src/components/Utils/$1",
      "@/(.*)$": "<rootDir>/src/components/$1",
      "@styles(.*)$": "<rootDir>/src/styles/$1",
      "@se(.*)$": "<rootDir>/src/components/Modules/SmartElements/$1"
    }
  }
}
